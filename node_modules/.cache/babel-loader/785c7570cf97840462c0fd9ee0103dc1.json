feedback\">\r\n                    <label for=\"product-kw\" class=\"sr-only\">搜索关键字：</label>\r\n                    <input v-model=\"kw\" @keyup.enter=\"loadPager(1)\" class=\"form-control\" type=\"text\" id=\"product-kw\" placeholder=\"请输入搜索关键字\">\r\n                    <span class=\"glyphicon glyphicon-search form-control-feedback\"></span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-12\">\r\n                <div class=\"table-responsive\">\r\n                    <table class=\"table table-hover table-striped\" id=\"table-laptop\">\r\n                        <thead>\r\n                        <tr>\r\n                            <th>\r\n                                <div class=\"checkbox\" style=\"margin: 0;\">\r\n                                    <label>\r\n                                        <input type=\"checkbox\">全选\r\n                                    </label>\r\n                                </div>\r\n                            </th>\r\n                            <th>编号</th>\r\n                            <th>图片</th>\r\n                            <th>型号</th>\r\n                            <th>主标题</th>\r\n                            <th>规格</th>\r\n                            <th>单价</th>\r\n                            <th>操作</th>\r\n                        </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                        <tr v-if=\"!pager.data\">\r\n                            <td colspan=\"8\">\r\n                                <div class=\"loading\">\r\n                                    <img src=\"img/loading.gif\" alt=\"\">\r\n                                </div>\r\n                            </td>\r\n                        </tr>\r\n                        <tr v-for=\"(l,i) in pager.data\">\r\n                            <td><input type=\"checkbox\"></td>\r\n                            <td>{{l.lid}}</td>\r\n                            <td><img class=\"pic\" :src=\"require('../assets/'+l.pic)\"></td>\r\n                            <td><p class=\"fname\" :title=\"l.fname\">{{l.fname}}</p></td>\r\n                            <td><p class=\"title\" :title=\"l.title\">{{l.title}}</p></td>\r\n                            <td><p class=\"spec\" :title=\"l.spec\">{{l.spec}}</p></td>\r\n                            <td>￥{{l.price}}</td>\r\n                            <td>\r\n                                <a href=\"product_details.html\">详情</a>\r\n                                <a href=\"product_update.html\">修改</a>\r\n                                <a href=\"product_delete.html\">删除</a>\r\n                            </td>\r\n                        </tr>\r\n                        </tbody>\r\n                    </table>\r\n                </div>\r\n                <div class=\"row\">\r\n                    <div class=\"col-xs-12\">\r\n                        <ul class=\"pagination pull-right\" id=\"pagination\">\r\n                            <li v-if=\"!(pager.pno==1)><a @click.prevent=\"loadPager(pager.pno-1)\" href=\"#\">上一页</a></li>\r\n                            <li v-if=\"pager.pno-2>0\"><a @click.prevent=\"loadPager(pager.pno-2)\" href=\"#\">{{pager.pno-2}}</a></li>\r\n                            <li v-if=\"pager.pno-1>0\"><a @click.prevent=\"loadPager(pager.pno-1)\" href=\"#\">{{pager.pno-1}}</a></li>\r\n                            <li class=\"active\"><a href=\"#\">{{pager.pno}}</a></li>\r\n                            <li v-if=\"pager.pno+1<=pager.pageCount\"><a @click.prevent=\"loadPager(pager.pno+1)\" href=\"#\">{{pager.pno+1}}</a></li>\r\n                            <li v-if=\"pager.pno+2<=pager.pageCount\"><a @click.prevent=\"loadPager(pager.pno+2)\" href=\"#\">{{pager.pno+2}}</a></li>\r\n                            <li v-if=\"!(pager.pno==pager.pageCount)\"><a @click.prevent=\"loadPager(pager.pno+1)\" href=\"#\">下一页</a></li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <br>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    export default {\r\n        data() {\r\n            return {\r\n                pager: {},\r\n                pageSize: 10,\r\n                kw: '',\r\n                downPage: true,\r\n                upPage: true\r\n            }\r\n        },\r\n        components: {},\r\n        methods: {\r\n            loadPager(pno){\r\n                var url = `http://127.0.0.1/VueJs/day06/xz_admin_v1/data/product_list.php?pno=${pno}&pageSize=${this.pageSize}&kw=${this.kw}`;\r\n                axios.get(url).then((res)=>{\r\n                    console.log(res);\r\n                    this.pager = res.data;\r\n                }).catch((err)=>{\r\n                    console.log(err);\r\n                })\r\n            }\r\n        },\r\n        computed: {},\r\n        watch: {\r\n            pageSize(){\r\n                this.loadPager(1);\r\n            }\r\n        },\r\n        mounted() {\r\n            this.loadPager(1);\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    #table-laptop p {\r\n        margin: 0;\r\n    }\r\n    #table-laptop td .title,\r\n    #table-laptop td .fname,\r\n    #table-laptop td .spec {\r\n        overflow: hidden;\r\n        white-space: nowrap;\r\n        text-overflow: ellipsis;\r\n    }\r\n    #table-laptop td .title {\r\n        width: 120px;\r\n    }\r\n    #table-laptop td .fname {\r\n        width: 80px;\r\n    }\r\n    #table-laptop td .spec {\r\n        width: 60px;\r\n    }\r\n    #table-laptop td .pic{\r\n        width: 40px;\r\n        vertical-align: middle;\r\n        position: absolute;\r\n        margin-top: -8px;\r\n    }\r\n</style>"],"sourceRoot":"src\\views"}]}